{% extends 'app.html.twig' %}

{% block title %}Register{% endblock %}

{% block content %}
    <h1>Register</h1>

    {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.username) }}
        {{ form_row(form.plainPassword) }}
        {{ form_row(form.termsAccepted) }}

        <button class="btn">Register</button>
    {{ form_end(form) }}
{% endblock %}
